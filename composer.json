{
    "name": "delisend/delisend-php-api",
    "description": "Delisend PHP REST API Library",
    "keywords": ["delisend", "api", "http", "rest", "web service", "curl", "client", "HTTP client", "marketing automation", "frauds protection"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://delisend.com/",
    "authors": [
        {
            "name": "Rosi Group s.r.o.",
            "email": "contact@rosigroup.com",
            "homepage": "https://rosigroup.com",
            "role": "Technical Support"
        },
        {
            "name": "Delisend",
            "email": "contact@delisend.com",
            "homepage": "https://delisend.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "guzzlehttp/psr7": "^2.4.3"
    },
    "require-dev": {
        "symfony/var-dumper": "5.4.x-dev"
    },
    "suggest": {
        "ext-curl": "Required for CURL handler support",
        "ext-intl": "Required for Internationalized Domain Name (IDN) support",
        "psr/log": "Required for using the Log middleware"
    },
    "autoload": {
        "psr-4": {
            "DelisendApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "prepend-autoloader": false,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
