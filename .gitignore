# Ignore IDE's configuration files
.idea/

# Ignore PHP CS Fixer local config and cache
.php_cs
.php_cs.cache

# Ignore PHPStan local config
.phpstan.neon

# Ignore phpDocumentor's local config and artifacts
.phpdoc/*
phpdoc.xml

